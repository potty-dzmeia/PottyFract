<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="my.potty.fract"
      android:versionName="1.9" android:versionCode="10">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-sdk android:targetSdkVersion="8" android:minSdkVersion="8"/>
    <supports-screens android:normalScreens="true" android:resizeable="true" android:anyDensity="true" android:largeScreens="true" android:smallScreens="true" android:xlargeScreens="true"/> 
      
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:allowBackup="true" android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" android:debuggable="false" android:restoreAnyVersion="true">

        <activity android:name=".ActivityMain" android:label="PottyFract" android:screenOrientation="landscape" android:icon="@drawable/icon">
            <intent-filter><action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
    </application>




</manifest> 